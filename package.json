{
  "name": "start-to-react",
  "private": true,
  "version": "0.0.0",
  "description": "A react starter kit to start your journey.",
  "keywords": [
    "react",
    "vite",
    "vitest",
    "playwright",
    "eslint",
    "prettier",
    "husky",
    "mui",
    "tailwindcss",
    "emotion",
    "zustand",
    "jotai"
  ],
  "homepage": "https://github.com/HamedFathi/StartToReact",
  "bugs": {
    "url": "https://github.com/HamedFathi/StartToReact/issues"
  },
  "license": "MIT",
  "author": "Hamed Fathi <hamedfathi.contact@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/HamedFathi/StartToReact"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "json": "json-server --watch db.json --port 3030",
    "lint": "eslint . --ext .tsx,.ts,.js,.jsx,.cjs,.mjs,.cts,.mts",
    "lint:fix": "eslint . --fix --ext .tsx,.ts,.js,.jsx,.cjs,.mjs,.cts,.mts",
    "prepare": "husky install",
    "commit": "cz",
    "release": "release-please",
    "changelog": "auto-changelog -p",
    "debug": "vite --port 4000"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@faker-js/faker": "^7.6.0",
    "@fontsource/roboto": "^4.5.8",
    "@hookform/resolvers": "^3.1.0",
    "@mdx-js/rollup": "^2.3.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@react-spring/web": "^9.7.2",
    "@tanstack/react-query": "^4.29.5",
    "chart.js": "^4.3.0",
    "dotenv": "^16.0.3",
    "goober": "^2.1.13",
    "i18next": "^22.4.15",
    "immutable": "^4.3.0",
    "jotai": "^2.0.4",
    "ky": "^0.33.3",
    "lodash": "^4.17.21",
    "material-react-table": "^1.11.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.2.2",
    "react-router-dom": "^6.11.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/json-server": "^0.14.4",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/coverage-istanbul": "^0.30.1",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "json-server": "^0.17.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "release-please": "^15.10.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.3.2",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4",
    "v8-to-istanbul": "^9.1.0",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  }
}
